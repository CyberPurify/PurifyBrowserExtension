{
    "name": "purify-browser-extension",
    "version": "0.5.9",
    "description": "Purify Browser Extension",
    "scripts": {
        "watch": "NODE_ENV=dev NODE_NO_WARNINGS=1 gulp buildWatch --require babel-register --gulpfile tasks",
        "dev": "yarn shipit && NODE_ENV=dev NODE_NO_WARNINGS=1 gulp buildDev --require babel-register --gulpfile tasks",
        "beta": "yarn shipit && NODE_ENV=beta gulp buildBeta --require babel-register --gulpfile tasks",
        "release": "NODE_ENV=release gulp buildRelease --require babel-register --gulpfile tasks",
        "sample-api": "NODE_ENV=dev gulp buildSampleApi --require babel-register --gulpfile tasks",
        "test": "gulp runTests --require babel-register --gulpfile tasks",
        "locales-download": "gulp updateLocalesStream --require babel-register --gulpfile tasks",
        "locales-upload": "gulp uploadLocalesStream --require babel-register --gulpfile tasks",
        "build-update": "gulp buildUpdatesFilesStream --require babel-register --gulpfile tasks",
        "resources": "gulp downloadResources --require babel-register --gulpfile tasks",
        "rebuild-locales": "gulp rebuildLocales --require babel-register --gulpfile tasks",
        "increment": "yarn version --patch --no-git-tag-version",
        "lint": "npx prettier --write src/**/*.{js,html,json} tasks/**/*.js tests/**/*.{js,html,json}",
        "lisence": "NODE_NO_WARNINGS=1 gulp addLisence --require babel-register --gulpfile tasks",
        "watch:BackgroundJs": "parcel watch src/lib/background-page/background.js --hmr-hostname localhost -d src/lib/background/ -o background.min.js --public-url ./",
        "watch:contentJs": "parcel watch src/lib/background-page/content.js --hmr-hostname localhost -d src/lib/background/ -o content.min.js --public-url ./",
        "build:BackgroundJs": "yarn build:backgroundJsMinify && yarn build:contentJs",
        "build:backgroundJsMinify": "parcel build src/lib/background-page/background.js -d src/lib/background/ -o background.min.js --public-url ./",
        "build:contentJs": "parcel build src/lib/background-page/content.js -d src/lib/background/ -o content.min.js --public-url ./",
        "shipit": "yarn buildPurifyjs && yarn scriptbundle && yarn minbundle",
        "buildPurifyjs": "tsc --skipLibCheck",
        "scriptbundle": "browserify ./src/lib/libs/purifyjs/purifyjs.js --standalone purifyjs --outfile ./src/lib/libs/purifyjs/bundle.js",
        "minbundle": "terser src/lib/libs/purifyjs/bundle.js --compress --mangle --output src/lib/libs/purifyjs.min.js && rm -r ./src/lib/libs/purifyjs/bundle.js"
    },
    "babel": {
        "presets": [
            "env"
        ],
        "plugins": [
            "transform-runtime",
            "transform-object-rest-spread"
        ],
        "babelrc": false
    },
    "repository": {
        "type": "git",
        "url": "ssh://git@github.com:CyberPurify/PurifyBrowserExtension.git"
    },
    "dependencies": {
        "@nsfw-filter/gif-frames": "1.0.2",
        "@tensorflow/tfjs-node": "^3.6.1"
    },
    "author": "cyberpurify@cyberpurify.com",
    "license": "MIT",
    "devDependencies": {
        "@tensorflow/tfjs": "^3.1.0",
        "@types/jest": "^26.0.0",
        "all-contributors-cli": "^6.1.2",
        "axios": "^0.19.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-runtime": "^6.26.0",
        "browserify": "^17.0.0",
        "checksum": "^0.1.1",
        "clang-format": "^1.2.3",
        "crx": "^5.0.1",
        "del": "^4.1.0",
        "event-stream": "^4.0.1",
        "form-data": "^2.5.0",
        "gulp": "^4.0.0",
        "gulp-concat": "^2.6.1",
        "gulp-download2": "^1.0.2",
        "gulp-file": "^0.4.0",
        "gulp-hash-creator": "^0.2.0",
        "gulp-javascript-obfuscator": "^1.1.6",
        "gulp-modify-file": "^1.0.1",
        "gulp-rename": "^2.0.0",
        "gulp-tap": "^2.0.0",
        "gulp-zip": "^4.1.0",
        "jest": "^26.6.3",
        "jpeg-js": "^0.4.0",
        "lodash": "^4.17.11",
        "node-fetch": "^2.3.0",
        "node-qunit-puppeteer": "^2.0.3",
        "parcel-bundler": "^1.12.5",
        "preprocess": "^3.1.0",
        "punycode": "^2.1.0",
        "qunit": "^2.6.0",
        "regenerator-runtime": "^0.13.7",
        "seedrandom": "~3.0.3",
        "terser": "^5.3.8",
        "ts-jest": "^26.5.5",
        "ts-node": "~9.1.0",
        "tslint": "~6.1.0",
        "typescript": "4.2.4",
        "uglify-js": "^3.13.6",
        "vinyl-sourcemaps-apply": "^0.2.1",
        "web-ext": "^2.6.0"
    },
    "keywords": [
        "NSFW",
        "JavaScript",
        "ML",
        "Machine",
        "Learning",
        "Tensorflow",
        "JS"
    ]
}